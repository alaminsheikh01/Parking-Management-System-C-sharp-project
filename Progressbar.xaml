<Window x:Class="FinalProject.Progressbar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinalProject"
        mc:Ignorable="d"
        Title="Progressbar" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush TileMode="FlipX" ImageSource="2.jpg" Opacity="0.66"/>
        </Grid.Background>
        <ProgressBar Minimum="0" Maximum="100" Name="pbStatus" Margin="0,349,0,0" IsIndeterminate="True" ValueChanged="pbStatus_ValueChanged" Grid.ColumnSpan="2" />



    </Grid>
</Window>
